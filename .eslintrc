{
  "parser": "babel-eslint",
  "extends": ["eslint-config-airbnb"],
  "plugins": ["import", "prettier"],
  "rules": {
    "global-require": 0,
    "react/sort-comp": 0,
    "no-mixed-operators": 0,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
